{
  "name": "koa-decorator-x",
  "version": "1.0.7",
  "description": "Koa-decorator is a package that provides a set of decorators to simplify the creation and management of routes in Koa.js applications.",
  "main": "./packages/index.js",
  "typings": "./packages/@types/index.d.ts",
  "types": "./packages/@types/index.d.ts",
  "author": "Harry Xiong",
  "license": "MIT",
  "keywords": [
    "koa",
    "decorator",
    "ios",
    "typescript",
    "node"
  ],
  "files": [
    "packages",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "node ./scripts/addTypeModule.js && cross-env NODE_ENV=production rollup -c && node ./scripts/removeTypeModule.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:prettier": "prettier --write \"**/*.{ts,tsx,js,json,html,yml,css,less,scss,md}\"",
    "lint:eslint": "eslint --fix .",
    "lint:stylelint": "stylelint --fix --config .stylelintrc.js **/*.{less,css,scss}",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "log:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@jest/types": "^29.6.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rollup": "^4.18.1",
    "rollup-plugin-uglify": "^6.0.4",
    "stylelint": "^16.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.1",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "reflect-metadata": "^0.2.2"
  }
}
